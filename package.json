{
  "name": "vite-vue3-tpl",
  "version": "0.0.0",
  "license": "ISC",
  "docker_package": "constq/page-blog:front-v1",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:force": "vite --mode dev --force",
    "build": "vue-tsc --noEmit && vite build --mode dev",
    "buildProd": "vue-tsc --noEmit && vite build --mode prod",
    "serve": "vite preview --mode prod",
    "prepare": "husky install",
    "test": "jest",
    "cz": "git add . && git cz",
    "docker:image": "docker build -t %npm_package_docker_package% .",
    "docker:push": "docker push %npm_package_docker_package%",
    "docker": "pnpm docker:image && pnpm docker:push",
    "release": "pnpm buildProd && pnpm docker"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.0",
    "axios": "^1.4.0",
    "axios-jsonp": "^1.0.4",
    "axios-retry": "^3.4.0",
    "element-plus": "^2.3.4",
    "pinia": "^2.0.35",
    "postcss": "^8.4.23",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@constq/eslint-config-qzr": "^0.0.5",
    "@constq/qzr-utils": "1.2.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.2",
    "@types/qs": "^6.9.7",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/test-utils": "^2.3.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "postcss-px-to-viewport": "^1.1.1",
    "qs": "^6.11.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "stylus": "^0.59.0",
    "ts-jest": "^29.1.0",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.3",
    "vite-plugin-compression": "^0.5.1",
    "vue-global-api": "^0.4.1",
    "vue-jest": "^3.0.7",
    "vue-tsc": "^1.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
